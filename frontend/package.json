{
    "name": "svelte-app",
    "version": "1.0.0",
    "private": true,
    "scripts": {
      "build": "rollup -c",
      "dev": "rollup -c -w",
      "start": "sirv public --no-clear --port 8081",
      "format": "prettier --write \"./**/*.html\" \"./**/*.svelte\"",
      "prepare": "husky install"
    },
    "devDependencies": {
      "@rollup/plugin-commonjs": "^22.0.0",
      "@rollup/plugin-node-resolve": "^13.3.0",
      "husky": "^8.0.3",
      "lint-staged": "^13.2.2",
      "prettier": "^2.8.8",
      "prettier-plugin-svelte": "^2.10.1",
      "rollup": "^2.74.0",
      "rollup-plugin-css-only": "^3.1.0",
      "rollup-plugin-livereload": "^2.0.5",
      "rollup-plugin-svelte": "^7.1.0",
      "rollup-plugin-terser": "^7.0.2",
      "svelte": "^3.48.0"
    },
    "dependencies": {
      "axios": "^0.27.2",
      "papaparse": "^5.4.1",
      "raw-loader": "^4.0.2",
      "sirv-cli": "^2.0.2",
      "svelte-particles": "^2.10.1",
      "svelte-routing": "^1.10.0",
      "svelte-spa-router": "^3.2.0",
      "svelte-use-form": "^2.6.2"
    },
    "lint-staged": {
      "*": "prettier --ignore-unknown --write"
    }
  }
  